    <script src="~/Content/css/nav-drawer/js/drawer.js"></script>
    <link href="~/Content/css/nav-three/nav-three.css" rel="stylesheet" />
    <script src="~/Content/css/nav-three/nav-three.js"></script>
    <style>
        .layui-nav-tree {
            width: 100%;
        }

        .layui-nav-item .layui-nav-child {
            background-color: #e6e6e6 !important;
            color: #727272 !important;
        }

        .layui-nav-tree .layui-nav-child a {
            color: #727272 !important;
            border-bottom: solid 1px #bfbfbf;
        }

        .drawer-navbar .drawer-navbar-header {
            border-bottom: none;
        }

        .drawer-open .drawer-navbar .drawer-navbar-header {
            border-bottom: 1px solid #fff;
        }

        .drawer-navbar .drawer-navbar-header {
            background-color: #012353;
        }

        .drawer-hamburger {
            width: 1.5rem;
            padding-right: 20px;
        }

        .drawer-hamburger-icon:before, .drawer-hamburger-icon:after {
            top: -8px;
        }

        .drawer-hamburger-icon:after {
            top: 8px;
        }



        .drawer-container {
            background-color: #ebe3e2;
            height: 3.75rem;
            line-height: 3.75rem;
        }

        .drawer-navbar-header {
            text-align: left;
        }

        .drawer-hamburger-icon, .drawer-hamburger-icon:before, .drawer-hamburger-icon:after {
            background-color: #fff;
        }

        .head-logo {
            width: 35vw;
            left: 0;
            vertical-align: middle;
        }

        .drawer-overlay {
            background-color: #012353;
        }

        /*ul.menu1 li .inactives:after {
            content: "-";
            position: absolute;
            font-size: 5vw;
            right: 5px;
        }

        ul.menu1 li .inactive:after {
            content: "+";
            position: absolute;
            font-size: 5vw;
            right: 5px;
        }*/
    </style>
</head>
